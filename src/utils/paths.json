{
  "compilerOptions": {
    "esModuleInterop": true,
    "paths": {
      "@app/*": ["src/*"],
      "@/*": ["./src/*"],
      "@store/*": ["src/store/*"],
      "@store": ["src/store/"],
      "@components": ["src/components/index"],
      "@modules/*": ["src/modules/*"],
      "@pages/*": ["src/pages/*"]
    }
  }
}
